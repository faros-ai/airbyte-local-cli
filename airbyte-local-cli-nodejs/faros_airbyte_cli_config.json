{
  "src": {
    "image": "farosai/airbyte-azure-workitems-source",
    "config": {
      "instance": {
        "type": "server",
        "api_url": "https://dev.azure.com"
      },
      "access_token": "MySecretToken",
      "organization": "org",
      "projects": [
        "my-projects"
      ]
    }
  },
  "dst": {
    "image": "farosai/airbyte-faros-destination",
    "config": {
      "edition_configs": {
        "api_url": "https://dev.api.faros.ai/",
        "api_key": "MySecretToken"
      },
      "origin": "azure-workitems"
    }
  }
}